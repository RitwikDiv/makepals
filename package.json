{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for makepals app which takes input from the user and creates a discord channel in the server. It automatics cleans up the purges empty rooms and monitors the behavior of members",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js --ignore 'client/'",
    "client": "npm run serve --prefix client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "backend",
    "discordjs",
    "channel monitor",
    "member management"
  ],
  "author": "Ritwik Divakaruni",
  "license": "ISC",
  "dependencies": {
    "bad-words": "^3.0.4",
    "cors": "^2.8.5",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http": "*",
    "joi": "^17.4.0",
    "mongoose": "^5.12.1",
    "morgan": "^1.10.0"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "concurrently": "^6.0.1"
  }
}
